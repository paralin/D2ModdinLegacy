.statusBar
  position fixed
  bottom 0
  left 0
  width 100%
  height 40px
  background-color rgb(0, 163, 255)
  box-shadow 0 0 30px 0px rgba(0, 204, 255, 0.46)
  line-height 40px
  z-index: 99999
.statusBar .left
  width: 50%
  height: 40px
  font-size 2.5em
  color white
  margin-left 10px
.statusBar .left .status
  font-size 20px
  position relative
  bottom 3px
  font-weight 400
.statusBar .rightButtons
  text-align right
  height: 40px
  position absolute
  right 5px
  line-height 40px
  bottom 0
